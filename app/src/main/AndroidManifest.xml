<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.swust.firstcold.diglettgame">

    <application
        android:allowBackup="true"
        android:icon="@drawable/mouse"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DiglettGame">
        <activity
            android:name=".rankingListActivity"
            android:exported="false" />

        <service
            android:name=".BcsoundService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".LevelSelectionActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />

        <service
            android:name=".BcmusicService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".MouseActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>